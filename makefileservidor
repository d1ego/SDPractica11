	g++ -c Respuesta.cpp
SocketDatagrama.o:
	g++ -c SocketDatagrama.cpp
PaqueteDatagrama.o:
	g++ -c PaqueteDatagrama.cpp
clean:
	rm Respuesta.o
	rm PaqueteDatagrama.o
	rm SocketDatagrama.o
	rm servidor
